{
  "compilerOptions": {
    "jsx": "react-jsx",
    "baseUrl": ".",
    "paths": {
      "tests/*": ["__tests__/*"],
      "app/*": ["src/app/*"],
      "pages/*": ["src/pages/*"],
      "store/*": ["src/store/*"],
      "components/*": ["src/components/*"],
      "widgets/*": ["src/widgets/*"],
      "hooks/*": ["src/hooks/*"],
      "config/*": ["src/config/*"],
      "errors/*": ["src/errors/*"],
      "entity/*": ["src/entity/*"],
      "assets/*": ["src/assets/*"],
